<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/add_team"
    android:gravity="center_vertical"
    android:minHeight="?android:attr/listPreferredItemHeight">

    <Space
        android:layout_width="@dimen/list_item_margin"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:src="@drawable/create_team" />

    <Space
        android:layout_width="@dimen/list_item_margin"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_team_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        android:textColor="@color/moxtra_blue"
        android:text="@string/Create_Team"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</LinearLayout><!-- From: file:/var/lib/jenkins/jobs/Build_2.9.2_Based_Chat_SDK/workspace/android/branches/2.9.2/resources/res/layout/row_add_team.xml -->